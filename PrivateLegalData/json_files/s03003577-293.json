                    "sentence_id": "0",
                    "paragraph_index": 11,
                    "global_sentence_index": 15
                },
                {
                    "sentence": "בתגובה למעשיו של הנאשם פרצה בצעקות",
                    "sentence_id": "1",
                    "paragraph_index": 11,
                    "global_sentence_index": 16
