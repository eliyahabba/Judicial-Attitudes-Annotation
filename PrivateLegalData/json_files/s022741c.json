            "sentences": [
                {
                    "sentence": "ה",
                    "sentence_id": "0",
                    "paragraph_index": 48,
                    "global_sentence_index": 65
                },
                {
                    "sentence": "אין ראיה לגבי הנזק שנגרם למתלוננת, מעבר להנחה שנגרם נזק",
                    "sentence_id": "1",
                    "paragraph_index": 48,
                    "global_sentence_index": 66
                }
            ]
        },
        {
            "paragraph_index": 49,
            "sentences": [
                {
                    "sentence": "ו",
                    "sentence_id": "0",
                    "paragraph_index": 49,
                    "global_sentence_index": 67
                },
                {
