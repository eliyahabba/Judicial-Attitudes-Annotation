        },
        {
            "paragraph_index": 4,
            "sentences": [
                {
                    "sentence": "התוצאה אליה הגעתי היא,  כי יש לזכות את הנאשם מחמת הספק",
                    "sentence_id": "0",
                    "paragraph_index": 4,
