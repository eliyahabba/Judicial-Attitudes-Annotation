        },
        {
            "paragraph_index": 33,
            "sentences": [
                {
                    "sentence": "הנאשם תפס בראשה בימינו, חיבק אתה ונשק לה",
                    "sentence_id": "0",
                    "paragraph_index": 33,
                    "global_sentence_index": 50
