                }
            ]
        },
        {
            "paragraph_index": 5,
            "sentences": [
                {
                    "sentence": "בפתח הדברים אני מודיעה כי החלטתי לזכות את הנאשם",
                    "sentence_id": "0",
                    "paragraph_index": 5,
                    "global_sentence_index": 5
                }
