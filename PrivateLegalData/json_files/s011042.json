                }
            ]
        },
        {
            "paragraph_index": 36,
            "sentences": [
                {
                    "sentence": "על פי  לחוק, האישום הינו בגין מעשה מגונה \"בפומבי\"",
                    "sentence_id": "0",
                    "paragraph_index": 36,
                    "global_sentence_index": 50
